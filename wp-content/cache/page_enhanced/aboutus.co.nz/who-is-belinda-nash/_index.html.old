    $("#now-reading").on('click', function(e){
        e.preventDefault();
        $(this).hide();
        $("#newsroom-navigation").find('.close').show();
        $("#newsroom-navigation").stop().animate({
	        height: fullHeight
        });
        $(".newsroom-feed, .newsroom-twitter").stop().animate({
